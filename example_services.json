{
    "proxyIP": "0.0.0.0",
    "serviceHostIP": "0.0.0.0",
    "resources": {
        "allocationLimits": {
            "mcpu": 4000,
            "memoryMi": 8000,
            "vmemoryMi": 4000
        }
    },
    "services": [
        {
            "name": "helloworld",
            "image": "strm/helloworld-http:latest",
            "pullPolicy": "ifnotpresent",
            "cooldown": 60,
            "priority": 1,
            "resources": {
                "cpu": 0,
                "memory": 0,
                "vmemory": 0
            },
            "ports": [
                {
                    "containerPort": 80,
                    "hostPorts": [80]
                }
            ]
        }
    ]
}